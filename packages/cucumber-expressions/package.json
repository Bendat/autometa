{
  "name": "@autometa/cucumber-expressions",
  "version": "0.3.2",
  "description": "wrapper on @cucumber/cucumber-expressions",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/strip-color": "^0.1.0",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "istanbul": "^0.4.5",
    "rimraf": "^4.1.2",
    "strip-color": "^0.1.0",
    "tsconfig": "workspace: *",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@autometa/asserters": "workspace:^",
    "@autometa/datetime": "workspace:^",
    "@autometa/errors": "workspace:^",
    "@autometa/overloaded": "workspace:^",
    "@autometa/types": "workspace:^",
    "@cucumber/cucumber-expressions": "^16.1.2",
    "@types/diff": "^5.0.3",
    "@types/luxon": "^3.3.1",
    "closest-match": "^1.3.3",
    "colors-cli": "^1.0.32",
    "diff": "^5.1.0",
    "luxon": "^3.3.0"
  }
}
