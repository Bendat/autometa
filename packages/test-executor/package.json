{
  "name": "@autometa/test-executor",
  "version": "0.4.0",
  "description": "Executes Autometa tests",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build": "tsup"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "dependencies": {
    "@autometa/app": "workspace:^",
    "@autometa/assertions": "workspace:^",
    "@autometa/bind-decorator": "workspace:^",
    "@autometa/dto-builder": "workspace:^",
    "@autometa/errors": "workspace:^",
    "@autometa/events": "workspace:*",
    "@autometa/gherkin": "workspace:*",
    "@autometa/phrases": "workspace:^",
    "@autometa/scopes": "workspace:*",
    "@autometa/types": "workspace:*",
    "@cucumber/cucumber-expressions": "^16.1.2",
    "@jest/reporters": "^29.5.0",
    "@types/glob": "^8.0.1",
    "glob": "^8.1.0",
    "tsup": "^6.7.0",
    "tsyringe": "^4.7.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "node": "^19.6.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
