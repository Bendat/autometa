    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "istanbul": "^0.4.5",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace: *",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@autometa/app": "workspace:^",
