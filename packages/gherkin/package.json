{
  "name": "@autometa/gherkin",
  "version": "0.5.5",
  "description": "Gherkin implementation for @autometa",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "author": "Ben Aherne <null@null.com>",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber-expressions": "^16.1.2",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "istanbul": "^0.4.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace: *",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@autometa/bind-decorator": "workspace:^",
    "@autometa/dto-builder": "workspace:*",
    "@autometa/errors": "workspace:^",
    "@autometa/overloaded": "workspace:^",
    "@autometa/types": "workspace:*",
    "@cucumber/gherkin": "^26.0.3",
    "@cucumber/messages": "^21.0.1",
    "@cucumber/tag-expressions": "^5.0.1",
    "uuid": "^9.0.1"
  }
}
