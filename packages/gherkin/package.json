{
  "name": "@autometa/gherkin",
  "version": "0.1.0",
  "main": "index.ts",
  "type": "module",
  "description": "Gherkin implementation for @autometa",
  "scripts": {
    "test": "jest",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "postbuild": "tsc-alias -p ./tsconfig.cjs.json && tsc-alias -p ./tsconfig.json",
    "build:libs": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:code": "rimraf dist/ && pnpm run build:libs && ./fixup",
    "build": "pnpm build:code && pnpm postbuild"
  },
  "author": "Ben Aherne <null@null.com>",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber-expressions": "^16.1.2",
    "@jest/globals": "^29.4.3",
    "@jest/types": "^29.4.3",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:^0.4.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@autometa/dto-builder": "workspace:*",
    "@autometa/types": "workspace:*",
    "@cucumber/gherkin": "^26.0.3",
    "@cucumber/messages": "^21.0.1",
    "@cucumber/tag-expressions": "^5.0.1",
    "uuid": "^9.0.0"
  }
}
