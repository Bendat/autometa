{
  "name": "@autometa/dto-builder",
  "version": "0.13.11",
  "description": "Define DTOs and Entities and automatically create a builder class.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit -p tsconfig.dev.json",
    "type-check:watch": "tsc --noEmit --watch -p tsconfig.dev.json",
    "build": "tsup && pnpm run build:types",
    "build:types": "tsc --build --force tsconfig.types.json",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "test:ui": "vitest --ui --passWithNoTests",
    "coverage": "vitest run --coverage --passWithNoTests",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "tsup-config": "workspace:*",
    "typescript": "^4.9.5",
    "vitest": "1.4.0"
  },
  "dependencies": {
    "@autometa/injection": "workspace:^",
    "@autometa/types": "workspace:^",
    "any-date-parser": "^1.5.4",
    "class-validator": "^0.14.0",
    "closest-match": "^1.3.3"
  }
}
