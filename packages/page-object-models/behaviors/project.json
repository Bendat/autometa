  "sourceRoot": "packages/page-object-models/behaviors/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/page-object-models/behaviors/**/*.ts"]
      }
    },
    "test": {
