  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
