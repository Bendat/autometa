    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace: *",
    "tsup-config": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
