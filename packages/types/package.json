{
  "name": "@autometa/types",
  "version": "0.3.1",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "author": "Ben Aherne",
  "license": "MIT",
  "type": "module",
  "description": "Common types and type utilities for Autometa",
  "scripts": {
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:js": "tsup",
    "build:dts:tsc": " tsc src/index.ts --declarationDir dist --declaration --emitDeclarationOnly",
    "build:dts": "pnpm build:dts:tsc",
    "build": "pnpm build:js && pnpm build:dts ",
    "build:watch": "tsup --watch"
  },
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace: *",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.8.3",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
