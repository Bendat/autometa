{
  "name": "@autometa/examples-jest-functions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest --config jest.unit.config.cjs",
    "test:watch": "jest --config jest.unit.config.cjs --watch",
    "features": "node ./scripts/run-features.mjs",
    "features:watch": "node ./scripts/run-features.mjs --watch",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "coverage": "node -e \"const fs=require('node:fs');fs.mkdirSync('coverage',{recursive:true});fs.writeFileSync('coverage/.keep','');\""
  },
  "dependencies": {
    "@autometa/assertions": "workspace:*",
    "@autometa/core": "workspace:*",
    "@autometa/cucumber-expressions": "workspace:*",
    "@autometa/executor": "workspace:*",
    "@autometa/gherkin": "workspace:*",
    "@autometa/http": "workspace:*",
    "@autometa/injection": "workspace:*",
    "@autometa/jest-executor": "workspace:*",
    "@autometa/jest-transformer": "workspace:*",
    "@autometa/runner": "workspace:*",
    "@cucumber/cucumber-expressions": "^17.1.0",
    "@cucumber/gherkin": "^28.0.0",
    "@cucumber/messages": "^24.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.11.18",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5"
  }
}
