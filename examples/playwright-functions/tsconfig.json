{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../configuration/tsconfig/package.config.json",
  "compilerOptions": {
    "types": ["node"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": false,
    "useUnknownInCatchVariables": false,
    "exactOptionalPropertyTypes": false,
    "paths": {}
  },
  "include": ["src/**/*.ts", "autometa.config.ts", "playwright.config.ts"],
  "exclude": ["dist", "node_modules"]
}
