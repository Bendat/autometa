{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["env", "CUCUMBER_FILTER_TAGS"],
  "pipeline": {
    "build": {
      "dependsOn": ["lint", "test", "^build"],
      "inputs": ["**/*.ts"],
      "outputs": ["dist/**"]
    },
    "build:docs": {},
    "integrate": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "inputs": ["**/*.ts"]
    },
    "dev": {
      "cache": false
    }
  }
}
