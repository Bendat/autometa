{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["env", "CUCUMBER_FILTER_TAGS"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**"],
      "outputs": ["dist/**"]
    },
    "it": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
