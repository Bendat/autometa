{
  "name": "@autometa/bind-decorator",
  "version": "0.5.0",
  "description": "Binds the this keyword of a method. Respectfully a fork of https://www.npmjs.com/package/bind-decorator",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "prettify": "prettier --config .prettierrc '**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "keywords": [
    "bind this keyword",
    "bind decorator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "istanbul": "^0.4.5",
    "node": "^19.6.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace: *",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "0.34.6"
  }
}
