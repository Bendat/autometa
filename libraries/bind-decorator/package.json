{
  "name": "@autometa/bind-decorator",
  "version": "0.4.1",
  "description": "Binds the this keyword of a method. Respectfully a fork of https://www.npmjs.com/package/bind-decorator",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "prettify": "prettier --config .prettierrc '**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:js": "tsup",
    "build:dts:tsc": "tsc --build --clean && tsc && tsup --entry.index out/index.d.ts --dtsOnly",
    "build:dts": "pnpm build:dts:tsc",
    "build": "pnpm build:js && pnpm build:dts ",
    "build:watch": "tsup --watch"
  },
  "keywords": [
    "bind this keyword",
    "bind decorator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "node": "^19.6.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "tsup": "^6.7.0"
  }
}
