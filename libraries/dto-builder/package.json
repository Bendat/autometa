{
  "name": "@autometa/dto-builder",
  "version": "0.11.1",
  "description": "Define DTOs and Entities and automatically create a builder class.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
