  "keywords": [
    "dto",
    "builder",
    "builder-pattern",
    "builder pattern",
    "dto-builder",
    "entity builder"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "exports": {
