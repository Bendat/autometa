{
  "name": "@autometa/dto-builder",
  "version": "0.9.2",
  "description": "Define DTOs and Entities and automatically create a builder class.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:js": "tsup",
    "build:dts:tsc": "tsc --build --clean && tsc && tsup --entry.index out/src/index.d.ts --dtsOnly",
    "build:dts": "pnpm build:dts:tsc",
    "build": "pnpm build:js && pnpm build:dts ",
    "build:watch": "tsup --watch"
  },
  "keywords": [
    "dto",
    "builder",
    "builder-pattern",
    "builder pattern",
    "dto-builder",
    "entity builder"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "devDependencies": {
    "@autometa/types": "workspace:^",
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "class-validator": "^0.14.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "node": "^19.6.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace: *",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@autometa/types": "workspace:^",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
