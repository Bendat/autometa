{
  "name": "@autometa/status-codes",
  "version": "0.3.1",
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "description": "Simple Library to simply HTTP status codes",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:js": "tsup",
    "build:dts:tsc": "tsc --build --clean && tsc && tsup --entry.index out/src/index.d.ts --dtsOnly",
    "build:dts": "pnpm build:dts:tsc",
    "build": "pnpm build:js && pnpm build:dts ",
    "build:watch": "tsup --watch"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "keywords": [
    "status codes",
    "http",
    "node",
    "http status",
    "status text"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "tsup": "^6.7.0",
    "node": "^19.6.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
