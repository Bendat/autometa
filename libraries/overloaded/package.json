{
  "name": "@autometa/overloaded",
  "version": "0.2.6",
  "main": "dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "description": "Function and method overloading made easy",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:js": "tsup",
    "build:dts:tsc": "tsc --build --clean && tsc && tsup --entry.index out/src/index.d.ts --dtsOnly",
    "build:dts": "pnpm build:dts:tsc",
    "build": "pnpm build:js && pnpm build:dts",
    "build:watch": "tsup --watch"
  },
  "author": "Ben Aherne",
  "license": "MIT",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "keywords": [
    "Method overloading",
    "Function overloading",
    "Overloading",
    "Pattern matching"
  ],
  "devDependencies": {
    "@types/git-diff": "^2.0.3",
    "@cucumber/cucumber-expressions": "^16.1.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/coverage-istanbul": "^0.31.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "@autometa/types": "workspace:*",
    "colors-cli": "^1.0.32",
    "git-diff": "^2.0.6",
    "myzod": "^1.10.0",
    "tsyringe": "^4.7.0"
  }
}
