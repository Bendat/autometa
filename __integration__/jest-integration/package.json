{
  "name": "@autometa/jest-integration",
  "version": "0.1.37",
  "description": "",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "integrate:impl": "jest __integration__/impl --runInBand",
    "integrate:transform": "jest __integration__/transform --runInBand",
    "integrate": "pnpm integrate:impl && pnpm integrate:transform",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "build": "exit 0",
    "build:watch": "tsup --watch"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "default": "./dist/esm/index.js",
    "types": "./dist/index.d.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@autometa/scopes": "workspace:^",
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.3.0",
    "istanbul": "^0.4.5",
    "rimraf": "^4.1.2",
    "tsconfig": "workspace: *",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "@autometa/app": "workspace:^",
    "@autometa/bind-decorator": "workspace:^",
    "@autometa/jest-transformer": "workspace:^",
    "@autometa/runner": "workspace:^",
    "@jest/reporters": "^29.6.4",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
