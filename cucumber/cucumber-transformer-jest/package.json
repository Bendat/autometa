{
  "name": "@autometa/cucumber-transformer-jest",
  "version": "0.5.5",
  "description": "Convert Cucumber .feature files into Autometa cucumber tests with jest",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bendat/autometa"
  },
  "scripts": {
    "test": "vitest run --passWithNoTests ",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "keywords": [
    "jest gherkin",
    "jest cucumber",
    "autometa jest",
    "jest",
    "cucumber"
  ],
  "homepage": "http://github.com/bendat/autometa",
  "author": "Ben Aherne",
  "license": "ISC",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js",
    "types": "./dist/index.d.cts"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@jest/types": "^29.4.3",
    "@types/babel__core": "^7.20.1",
    "@types/glob": "^8.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "babel-preset-jest": "^29.5.0",
    "eslint": "^8.37.0",
    "eslint-config-custom": "workspace:*",
    "istanbul": "^0.4.5",
    "jest": "^29.5.0",
    "node": "^19.6.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "dependencies": {
    "@autometa/cucumber-runner": "workspace:^0.10.2",
    "@babel/core": "^7.22.11",
    "vitest": "0.34.6"
  }
}
