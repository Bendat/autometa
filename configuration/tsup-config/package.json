{
  "name": "tsup-config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Shared TSUP configuration for Autometa packages",
  "main": "index.js",
  "scripts": {
    "type-check": "node -e \"process.exit(0)\"",
    "coverage": "node -e \"const fs=require('node:fs');fs.mkdirSync('coverage',{recursive:true});fs.writeFileSync('coverage/.keep','');\""
  },
  "exports": {
    ".": "./index.js",
    "./base": "./base.js"
  },
  "files": [
    "index.js",
    "base.js",
    "index.d.ts"
  ],
  "dependencies": {
    "tsup": "^7.2.0"
  },
  "peerDependencies": {
    "tsup": ">=6.0.0"
  }
}
